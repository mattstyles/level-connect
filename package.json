{
  "name": "level-connect",
  "version": "0.6.1",
  "description": "Connect to a multi handle sublevel enabled db over http",
  "main": "index.js",
  "bin": {
    "connect": "bin/connect"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build",
    "clean": "rm -rf lib && mkdir lib",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir lib",
    "watch": "babel src --out-dir lib --watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mattstyles/level-connect.git"
  },
  "homepage": "https://github.com/mattstyles/level-connect",
  "keywords": [
    "leveldb",
    "level",
    "http",
    "level-party",
    "level-sublevel",
    "sublevel",
    "party"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "author": {
    "name": "Matt Styles",
    "email": "matt@veryfizzyjelly.com",
    "url": "http://veryfizzyjelly.com"
  },
  "license": "MIT",
  "dependencies": {
    "JSONStream": "^1.0.4",
    "co-body": "^2.0.0",
    "denodeify": "^1.2.1",
    "eventemitter3": "^1.1.1",
    "koa": "^1.0.0",
    "koa-bunyan-log": "^1.0.0",
    "koa-router": "^5.1.2",
    "level-party": "^2.1.2",
    "level-promisify": "^2.0.0",
    "level-sublevel": "^6.5.1",
    "node-uuid": "^1.4.3"

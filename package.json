{
  "name": "level-connect",
  "version": "0.7.0",
  "description": "Connect to a multi handle sublevel enabled db over http",
  "main": "index.js",
  "bin": {
    "connect": "bin/connect"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build",
    "clean": "rm -rf lib && mkdir lib",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir lib",
    "watch": "babel src --out-dir lib --watch"
  },
  "babel": {
    "presets": [
      "node5"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mattstyles/level-connect.git"
  },
  "homepage": "https://github.com/mattstyles/level-connect",
  "keywords": [
    "leveldb",
    "level",
    "http",
    "level-party",
    "level-sublevel",
    "sublevel",
    "party"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "author": {
    "name": "Matt Styles",
    "email": "matt@veryfizzyjelly.com",
    "url": "http://veryfizzyjelly.com"
  },
  "license": "MIT",
  "dependencies": {
    "JSONStream": "^1.1.1",
    "co-body": "^4.0.0",
    "koa": "^2.0.0-alpha.3",
    "koa-bunyan-log": "^2.0.1",
    "koa-router": "^7.0.1",
    "level-party": "^3.0.4",
    "level-sublevel": "^6.5.4",
    "node-uuid": "^1.4.7",
    "then-levelup": "github:mattstyles/then-levelup"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-node5": "^10.9.0"
  }
}

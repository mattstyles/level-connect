{
  "name": "level-connect",
  "version": "0.7.0",
  "description": "Connect to a multi handle sublevel enabled db over http",
  "main": "index.js",
  "bin": {
    "connect": "bin/connect"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build",
    "clean": "rm -rf lib && mkdir lib",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir lib",
    "watch": "babel src --out-dir lib --watch"
  },
  "babel": {
    "presets": [
      "es2015-node4"
    ],
    "plugins": [
      "transform-async-to-generator"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mattstyles/level-connect.git"
  },
  "homepage": "https://github.com/mattstyles/level-connect",
  "keywords": [
    "leveldb",
    "level",
    "http",
    "level-party",
    "level-sublevel",
    "sublevel",
    "party"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "author": {
    "name": "Matt Styles",
    "email": "matt@veryfizzyjelly.com",
    "url": "http://veryfizzyjelly.com"
  },
  "license": "MIT",
  "dependencies": {
    "JSONStream": "^1.0.7",
    "co-body": "^4.0.0",
    "denodeify": "^1.2.1",
    "eventemitter3": "^1.1.1",
    "koa": "^2.0.0-alpha.3",
    "koa-bunyan-log": "^2.0.1",
    "koa-router": "^5.4.0",
    "level-party": "^3.0.4",
    "level-promisify": "^2.0.1",
    "level-sublevel": "^6.5.4",
    "node-uuid": "^1.4.7"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-preset-es2015-node4": "^2.0.3"
  }
}
